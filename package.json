{
  "name": "spacex-graphql",
  "version": "1.0.0",
  "description": "App listing Spacex Missions",
  "main": "src/server.js",
  "private": true,
  "author": "Jon Fossheim",
  "license": "MIT",
  "workspaces": [
    "client"
  ],
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/netlify-lambda serve src",
    "server": "nodemon  src/server.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn server\" \"yarn client\" ",
    "build": "NODE_ENV=production ./node_modules/.bin/netlify-lambda build src && cd client && yarn build"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "client": "1.0.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "7.0.2",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "netlify-lambda": "^1.6.3",
    "serverless-http": "^2.5.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
